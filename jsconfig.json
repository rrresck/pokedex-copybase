{
    "vue/multi-word-component-names": {
      "ignores": ["Stats"]
    },

  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "allowJs": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },

  "vueCompilerOptions":
  {
    "extensions": [
      ".vue",
    ],
    "target": 2.7
  },
}
